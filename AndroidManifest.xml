<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tutlane.loginexample">
    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_size_invert"
        android:label="Learn Together"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.myapplication1.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.myapplication1.RegistrationActivity" android:label="Registration"> </activity>
        <activity android:name="com.example.myapplication1.Home" android:label="Home"> </activity>
        <activity android:name="com.example.myapplication1.Chat" android:label="Chat"> </activity>
        <activity android:name="com.example.myapplication1.Profile" android:label="Profile"> </activity>
        <activity android:name="com.example.myapplication1.Add" android:label="Add"> </activity>
        <activity android:name="com.example.myapplication1.Search" android:label="Search"> </activity>
        <activity android:name="com.example.myapplication1.List" android:label="List"> </activity>
        <activity android:name="com.example.myapplication1.English" android:label="English"> </activity>


    </application>
</manifest>